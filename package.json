{
  "name": "@mexl/backstage-plugin-catalog-backend-module-mcp",
  "version": "1.0.0",
  "description": "Backstage catalog backend module for MCP (Model Context Protocol) entities",
  "main": "dist/index.cjs.js",
  "types": "dist/index.d.ts",
  "license": "Apache-2.0",
  "author": "mexl",
  "repository": {
    "type": "git",
    "url": "https://github.com/mexl/backstage-plugin-mcp.git"
  },
  "homepage": "https://github.com/mexl/backstage-plugin-mcp#readme",
  "bugs": {
    "url": "https://github.com/mexl/backstage-plugin-mcp/issues"
  },
  "keywords": [
    "backstage",
    "plugin",
    "mcp",
    "model-context-protocol",
    "catalog",
    "ai",
    "llm",
    "entity"
  ],
  "backstage": {
    "role": "backend-plugin-module"
  },
  "packageManager": "yarn@4.4.1",
  "engines": {
    "node": ">=20.18.1"
  },
  "scripts": {
    "build": "yarn tsc && yarn build:backstage",
    "build:backstage": "backstage-cli package build",
    "tsc": "tsc --project tsconfig.declarations.json",
    "tsc:check": "tsc --noEmit",
    "lint": "echo 'Linting skipped - no ESLint config'",
    "test": "backstage-cli package test --passWithNoTests --watchAll=false",
    "clean": "backstage-cli package clean",
    "prepack": "yarn build"
  },
  "dependencies": {
    "@backstage/backend-plugin-api": "^1.4.1",
    "@backstage/catalog-model": "^1.7.5",
    "@backstage/plugin-catalog-node": "^1.17.2",
    "@backstage/types": "^1.1.1"
  },
  "devDependencies": {
    "@backstage/cli": "^0.33.1",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "@backstage/backend-plugin-api": "^1.4.1",
    "@backstage/catalog-model": "^1.7.5",
    "@backstage/plugin-catalog-node": "^1.17.2"
  },
  "files": [
    "dist",
    "examples",
    "README.md",
    "CHANGELOG.md"
  ],
  "publishConfig": {
    "access": "public"
  }
}
